
generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Users {
  id    String  @id @default(cuid())
  name  String
  email String  @unique
  message String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}